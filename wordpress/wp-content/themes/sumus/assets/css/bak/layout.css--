	font-size: 14px;
	font-size: 1.4rem;
	color: #999;
}
.openhouse-container h3 {
	margin-top: 0.5em;
	font-weight: bold;
	font-size: 18px;
	font-size: 1.8rem;
}
.openhouse-container .date {
	font-size: 18px;
	font-size: 1.8rem;
	color: #cf3e2b;
	font-weight: bold;
}
.openhouse-container .time {
	color: #cf3e2b;
	font-weight: bold;
	margin-bottom: 0.5em;
}
.openhouse-container .add {
