@charset "utf-8";

/* 1. 共通
==================================================================================== */

/* レイアウト
------------------------------------------------------------------------------- */
.ground {
	background-color: #fff;
}
.ground.sitetop {
	filter: alpha(opacity=0);
	-moz-opacity: 0;
	opacity: 0;
}
.ground.sub {
	margin-top: 66px;
}
.section {
	position: relative;
	margin: 3em 0;
}
/* Bootostrap base setting remix */
.container.none-margin {
	padding-left: 0;
	padding-right: 0;
}


/* 見出し
------------------------------------------------------------------------------- */
.section-title {
	width: 90%;
	text-align: center;
	margin: 0 auto 3em;
}
.section-title.partner {
	width: 90%;
	text-align: center;
	margin: 0 auto 3em;
}
.section-title.none-mb {
	margin: 0 auto 0;
}

/* 見出し01 下線があるコンテンツの見出し */
.heading-01 {
	display: inline-block;
	margin-bottom: 1.5em;
	padding: 0 0 0.2em 0;
	border-bottom: 2px solid #595757;
	font-size: 21px;
	font-size: 2.1rem;
	font-weight: bold;
}
.heading-01.white {
	border-bottom: 2px solid #fff;
}
.heading-01.none-mb {
	margin-bottom: 0;
}

/* 見出し大 色付き */
.heading-03 {
	margin-bottom: 1em;
	font-size: 22px; font-size: 2.2rem;
	font-weight: bold;
	border-bottom: none;
	color: rgba(167, 188, 10, 1);
}
.content-title p.read {
	font-size: 14px;
	font-size: 1.4rem;
	line-height: 2;
}

body#contact .heading-03 {
	font-size: 20px; font-size: 2.0rem;
	text-align: left;
	margin-bottom: 1.5em !important;
}
	
/* マーカースタイル */
span.marker {
	background-color: #fff7d7;
}


/* ボタンスタイル
------------------------------------------------------------------------------- */
.button {
	background-color: #fff;
	border: 2px solid #666;
	color: #666;
}
.button:hover {
	background-color: #666;
	border: 2px solid #666;
	color: #fff;
}
.button.style02 {
	background-color: #d24f32;
	border: 2px solid #d24f32;
	color: #fff;
}
.button.style02:hover {
	background-color: #fff;
	border-color: #d24f32;
	color: #d24f32;
}
.button.style03 {
	background-color: #ff7d1f;
	border: 2px solid #ff7d1f;
	color: #fff;
}
.button.style03:hover {
	background-color: #fff;
	border-color: #ff7d1f;
	color: #ff7d1f;
}
.button.style04 {
	background-color: #ff7d1f;
	border: 2px solid #ff7d1f;
	color: #fff;
}
.button.style04:hover {
	background-color: #fff;
	border-color: #ff7d1f;
	color: #ff7d1f;
}

.button.style05 {
	background-color: #CAC1A2;
	border: 2px solid #CAC1A2;
	color: #fff;
}
.button.style05:hover {
	background-color: #E6E2D6;
	border-color: #E6E2D6;
	color: #fff;
}

.button.style05.end {
	background-color: #aaaaaa;
	border: 2px solid #aaaaaa;
	color: #fff;
}
.button.style05.end:hover {
	background-color: #aaaaaa;
	border-color: #aaaaaa;
	color: #fff;
}



/* リード部分
------------------------------------------------------------------------------- */
/* 新 */
.l-read-top {
	padding: 2.5em 0;
}
.l-section-title {
	padding: 1em 0 2.5em;
}
.l-read-top-inner,
.l-section-title-inner {
	width: 90%;
	margin: 0 auto;
	text-align: center;
}
.l-read-top-inner p,
.l-section-title-inner p {
	font-size: 16px;
	font-size: 1.6rem;
	line-height: 2;
	text-align: left;
}
.l-read-top-inner.kodawari p {
	text-align: center;
	margin-bottom: 1em;
}

/* 削除可 */
.read-container {
	margin: 2em 0 2em;
}

body#works .read-container {
	margin: 2em 10px 2em;
}

.read-container h2 {
	font-size: 24px; font-size: 2.4rem;
	font-weight: bold;
	margin-bottom: 1.3em;
	text-align: center;
}
.read-container p {
	font-size: 14px; font-size: 1.4rem;
	line-height: 2;
	margin-bottom: 1em;
}
body#works .read-container p {
	text-align: left;
}
.read-container h2.workshop {
	font-size: 28px;
	font-size: 2.8rem;
	font-weight: normal;
}
.read-container h2.workshop span {
	display: block;
	padding: 0;
	font-size: 15px;
	font-size: 1.5rem;
	color: #777;
}

/* 下層メインビジュアル
------------------------------------------------------------------------------- */
.l-mainvisual-sub {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	justify-content: center;
	height: 100%;
}
.l-mainvisual-sub {
	position: relative;
	width: 100%;
	height: 170px;
	background-size: cover;
}
body#blog .l-mainvisual-sub,
body#workshop .l-mainvisual-sub,
body#sumusmarche .l-mainvisual-sub {
	width: 100%;
	height: 100px;
	background-size: contain;
}
.mainvisual-sub-txt h2 {
	font-size: 26px;	font-size: 2.6rem;
	font-weight: 500;
	text-align: center;
	letter-spacing: 0.1em;
	color: white;
	text-transform: uppercase;
	text-shadow: rgba(0, 0, 0, 0.1) 1px 1px 0;
}
.mainvisual-sub-txt h2 span {
	display: block;
	font-size: 14px;
	font-size: 1.4rem;
	letter-spacing: 0;
	font-weight: normal;
}
.mainvisual-sub-txt h2.re {
	color: #595757;
	text-shadow: rgba(0, 0, 0, 0.1) 0 0 0;
}

/* 削除可?? */
.mainvisual {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	justify-content: center;
	height: 100%;
}
.mainvisual {
	position: relative;
	width: 100%;
	height: 120px;
	background-size: cover;
}
.mainvisual>.content-title {
	margin: 0 auto;
}


/* 2. トップページ
==================================================================================== */
.slider-banner {
	position: absolute;
	right: 30px; bottom: 35px;
	z-index: 8888;
	width: 220px;
}

/* ニュース
------------------------------------------------------------------------------- */
.news {
	z-index: 8888;
	position: absolute;
	bottom: 0;
	display: block;
	overflow: hidden;
	width: 100%;
	font-size: 14px;
	font-size: 1.4rem;
}
.l-news {
	z-index: 9999;
	background-color: rgba(0, 0, 0, 0.6);
}
.l-news-inner {
	position: relative;
	width: 100%;
	margin: 0 auto;
	color: #fff;
	opacity: 1;
}
.l-news-inner>dl {
	position: relative;
	width: 100%;
	margin: 0 auto;
	text-align: left;
	color: #fff;
}
.l-news-inner>dl dt {
	float: none;
	padding: 0.5em 1em 0;
	clear: left;
}
.l-news-inner>dl dd {
	float: none;
	width: 95%;
	padding: 0.2em 1em 0.5em;
}

.nakazawa-banner {
	display: none;
	width: 100%;
	margin: 15px auto 0;
}

@media screen and (max-width:768px) {
.slider-banner {
	position: absolute;
	right: 10px;
	bottom: 90px;
	width: 120px;
}
.nakazawa-banner {
	display: none;
}

@media screen and (max-width: 480px) {

	.slider-banner {
	}

	.nakazawa-banner {
		display: block;
	}
}



.l-news-inner>dl dt {
	float: left;
	padding: 1em 1em 0.6em;
	clear: left;
	border-right: 1px solid rgba(255, 255, 255, 0.5);
}
.l-news-inner>dl dd {
	float: left;
	width: 85%;
	padding: 1em 1em 0.6em;
}
}

.l-news-inner a {
	color: #fff;
	text-decoration: none;
}
.l-news-inner a:hover,
.l-news-inner a:focus {
	color: #fff;
	text-decoration: underline;
}
.ticker {
	position: relative;
	overflow: hidden;
	margin: 0 auto;
	padding: 0;
	width: 100%;
	text-align: left;
}
.ticker ul {
	width: 100%;
	position: relative;
}
.ticker ul li {
	width: 100%;
	display: none;
}
.ticker ul li span {
	padding: 0 1em 0 0;
}


/* トップページ > スライドバナーエリア
------------------------------------------------------------------------------- */
.section.slid-banner {
	margin: 0;
	padding: 3em 0 1em;
	background: #f4f2e9;
}
.section.slid-banner .container {
	margin:  0 auto 3em;
	padding: 0;
	width: 95%;
}


/* トップページ > 一日展
------------------------------------------------------------------------------- */
.top-openhouse {
	display: none;	
}
.section.openhouse {
	padding: 3em 0;
	margin-bottom: 0;
	background: #f4f2e9;
}
.openhouse-container {
	position: absolute;
	background: rgba(255, 255, 255, 0.9);
	top: 0;
	right: 3%;
