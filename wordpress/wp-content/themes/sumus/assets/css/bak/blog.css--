/* シングルページ > テーブル 共通
------------------------------------------------------------------------------- */
.post-single table {
	border-collapse: collapse;
	text-align: left;
	font-size: 13px;
	font-size: 1.3rem;
	line-height: 1.5;
	border-top: 1px solid #ccc;
	border-left: 1px solid #ccc;
	margin-top: 2em;
	margin-bottom: 0.5em;
}

.post-single table th {
	width: 25%;
	padding: 10px;
	font-weight: normal;
	vertical-align: top;
	border-right: 1px solid #ccc;
	border-bottom: 1px solid #ccc;
}

.post-single table td {
	width: 80%;
	padding: 10px;
	vertical-align: top;
	border-right: 1px solid #ccc;
	border-bottom: 1px solid #ccc;
}

/* wp-pagenaviの設定
------------------------------------------------------------------------------- */
.wp-pagenavi {
	clear: both;
	text-align:center;
	margin-bottom: 1em;
}
.wp-pagenavi a,
.wp-pagenavi span {
	color: #666;
	background-color: #ffffff;
	border: 1px solid #bdbdbd;
	padding: 8px 15px;
	margin: 0 2px;
	white-space: nowrap;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	-webkit-transition: 0.2s ease-in-out;
	-moz-transition: 0.2s ease-in-out;
	-o-transition: 0.2s ease-in-out;
	transition: 0.2s ease-in-out;
	text-align: center;
	text-decoration: none;
}
.wp-pagenavi a:hover{
	color: #a7bc0a;
	background-color: #fff;
	border-color: #a7bc0a;
}
.wp-pagenavi span.current{
	color: #fff;
	background-color: #a7bc0a;
	border-color: #a7bc0a;
	font-weight: bold;
}


/* ページ送り
------------------------------------------------------------------------------- */
.post-single .pagination {
	width: 100%;
	margin: 3em auto 5em;
	border-top: 1px solid #ddd;
	padding: 4em 0 0;
	font-size: 13px;
	font-size: 1.3rem;
}

@media screen and (min-width:768px) {
.post-single .pagination {
	margin: 3em auto 1em;
	padding: 3em 0 0;
}
}

.post-single .pagination ul {
	padding: 0 0.5%
}

.pagination ul li a {
	color: #666;
	background-color: #ffffff;
	border: 1px solid #bdbdbd;
	padding: 8px 15px;
	margin: 0 2px;
	white-space: nowrap;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	-webkit-transition: 0.2s ease-in-out;
	-moz-transition: 0.2s ease-in-out;
	-o-transition: 0.2s ease-in-out;
	transition: 0.2s ease-in-out;
	text-align: center;
	text-decoration: none;
}
.pagination ul li a:hover{
	color: #a7bc0a;
	background-color: #fff;
	border-color: #a7bc0a;
}
.pagination span.current{
	color: #fff;
	background-color: #a7bc0a;
	border-color: #a7bc0a;
	font-weight: bold;
}



.post-single .pagination .prev {
	float: left;
	width: 44%;
	text-align: left;
	word-break: break-all;
	line-height: 1.5em
}
.post-single .pagination .next {
	float: right;
	width: 44%;
	text-align: right;
	word-break: break-all;
	line-height: 1.5em
}
.post-single .pagination .navitop {
	float: left;
	width: 12%;
	text-align: center;
	line-height: 1.5em;
}
.post-single .pagination .page-navi {
  margin: 0 0 5em 0
}

/* ウィジエット カレンダーのスタイル
==================================================================================== */
#wp-calendar {
width:100%;
font-size:12px;
font-weight:300;
border-collapse:separate;
border-spacing:8px;
margin-left:-2px;
}

#wp-calendar
caption {
display:none;
}

#wp-calendar tbody
td {
margin-bottom:10px;
text-align:center;
-webkit-border-radius:2px;
-moz-border-radius:2px;
-ms-border-radius:2px;
-o-border-radius:2px;
border-radius:2px;
border:1px solid #f4f4f4;
padding:6px;
}

#wp-calendar tbody td:hover {
border-color:#e2e2e2;
}

#wp-calendar tbody
td.pad {
border:0;
}

#wp-calendar tbody
td#today {
color:#47C2DC;
border-color:#47C2DC;
}

#wp-calendar tfoot td#prev a,#wp-calendar tfoot td#next
a {
color:#888;
font-size:12px;
font-weight:400;
position:relative;
text-transform:uppercase;
bottom:-9px;
margin-bottom:1px;
}

#wp-calendar tfoot
td#prev {
text-align:left;
}

#wp-calendar tfoot
td#next {
text-align:right;
}

#wp-calendar thead
th {
font-size:13px;
font-weight:400;
color:#888;
text-align:center;
padding-bottom:10px;
}

#wp-calendar tbody td
a,#wp-calendar tfoot td#prev a:hover,#wp-calendar tfoot td#next a:hover {
color:#47C2DC;
